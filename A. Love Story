link - https://codeforces.com/contest/1829/problem/A


import java.util.*;

public class Main{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while(t-->0){
            String s=sc.next();
            int ans=0;
            String str = "codeforces";
            for(int i=0;i<10;i++){
                if(s.charAt(i)!=str.charAt(i)) ans++;
            }
            System.out.println(ans);
        }
        sc.close();   
    }
}


