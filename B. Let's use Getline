#include<bits/stdc++.h>
using namespace std;

int main(){
    string s;
    getline(cin, s);

    int j=s.find("\\"); //Note we will have to use two \ becuase a single \ means to leave it out in C++ - its an escape sequence character

    for(int i=0;i<j;i++){
        cout<<s.at(i);
    }
    
    return 0;
}



