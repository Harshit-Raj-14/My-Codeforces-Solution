#include<bits/stdc++.h>
#include <string>
using namespace std;

int main(){
    int a,b;
    char s;
    cin>>a>>s>>b;
    if(s=='+') cout<<a+b;
    if(s=='-') cout<<a-b;
    if(s=='/') cout<<a/b;
    if(s=='*') cout<<a*b;

    return 0;
  
}


