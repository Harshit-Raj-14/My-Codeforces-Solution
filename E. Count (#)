#include<bits/stdc++.h>
#include <string>
using namespace std;

int main(){
    string str;
    cin>>str;

    long long sum=0;

    for (int i=0;i<str.length();i++){
        sum+=int(str[i]) - 48;
    }

    cout<<sum;

    return 0;
}

    
    
